@model MVC8amPanthers.Models.Student
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<form action="/Default/Edit" method="post">
    <input type="hidden" value="@Model.StudId" name="StudId" />
    <label>Student Name</label>
    <input type="text" id="Stname" name="StudName" value="@Model.StudName" class="form-control" /><br />
    <label>Fees</label>
    <input type="text" id="StFees" name="StudFees" value="@Model.StudFees" class="form-control" /><br />
    <label>Section</label>
    <input type="text" id="StSection" name="Section" value="@Model.Section" class="form-control" /><br />
    <input type="submit" value="submit" class="btn btn-success" />
</form>
